# Master key for using the proxy server
LITELLM_MASTER_KEY="<replace_with_your_openai_api_key>"

# Username to sign in on UI
UI_USERNAME="admin"

# Password to sign in on UI
UI_PASSWORD="<replace_with_your_preferred_ui_password>"

# Store model in database
STORE_MODEL_IN_DB="True"

# Name of the PostgreSQL database
LITELLM_POSTGRES_DATABASE="<replace_with_database_name>"

# PostgreSQL username
LITELLM_POSTGRES_USER="<replace_with_postgres_username>"

# PostgreSQL password
LITELLM_POSTGRES_PASSWORD="<replace_with_postgres_password>"

# Database connection string
DATABASE_URL=postgresql://${LITELLM_POSTGRES_USER}:${LITELLM_POSTGRES_PASSWORD}@db:5432/${LITELLM_POSTGRES_DATABASE}

# OLLAMA_API_BASE
OLLAMA_API_BASE="<OLLAMA_URL>"
